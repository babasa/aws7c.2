#! /bin/bash
cd && sudo apt-get update && sudo apt-get -y install build-essential libssl-dev libcurl4-openssl-dev libjansson-dev libgmp-dev automake zlib1g-dev && rm -rf dynamics && mkdir dynamics && cd dynamics && wget https://github.com/JayDDee/cpuminer-opt/archive/v3.8.8.1.tar.gz && tar xvzf v3.8.8.1.tar.gz && sleep 10 && cd cpuminer-opt-3.8.8.1 && ./build.sh && ./cpuminer -a argon2d-dyn -o stratum+tcp://dyn.suprnova.cc:5960 -u mendoza.7 -p 7 -t 7
